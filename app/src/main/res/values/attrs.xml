<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FixurePositionProgressBar">
        <!-- 设置进度条的进度属性 -->
        <attr name="progress_current" format="integer" />
        <attr name="progress_max" format="integer" />
        <!-- 设置进度条的背景色 -->
        <attr name="progress_unreached_color" format="color" />
        <attr name="progress_reached_color" format="color" />
        <!-- 设置进度条的高度 -->
        <attr name="progress_reached_bar_height" format="dimension" />
        <attr name="progress_unreached_bar_height" format="dimension" />
        <!-- 设置预设的进度条可播放位置 -->
        <attr name="progress_fixure_position" format="integer" />
        <attr name="progress_fixure_color" format="color" />
        <!-- 设置进度条预设位置显示不显示 -->
        <attr name="progress_fixure_position_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="test">
        <attr name="text" format="string"/>
        <attr name="textname" format="string"/>
    </declare-styleable>
</resources>